citHeader("To cite package 'EQL' in publications use:")

year <- 2009
vers <- paste("R package version", meta$Version)
school <- paste("Technische Universit", "\\", "\"{a}t Graz", sep="")
school2 <- "Technische Universitaet Graz"
title <- "Die Extended-Quasi-Likelihood-Funktion in Generalisierten Linearen Modellen"
citEntry(entry="mastersthesis",
         title = title,
         author = person("Thorn Thaler"),
         year = year,
         school = school,
         textVersion = paste("Th. Thaler: ", title, ", ", school2, ", master's thesis.", sep=""))
